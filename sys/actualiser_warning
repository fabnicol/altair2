#!/sbin/openrc-run
# $Id$


start() {

while true
do
    a=$(rc-status | grep ajuster_version | grep starting)

    if test "y$a" != "y"; then
    chmod +x  /home/fab/Dev/altair/linux/warning
    exec /home/fab/Dev/altair/linux/warning
    fi
    sleep 30
done
}

stop() {

kill -9 $(pidof warning)

}
